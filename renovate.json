{
  "extends": ["config:base", ":masterIssue"],
  "labels": ["dependencies"],
  "bumpVersion": "patch",
  "updateInternalDeps": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "force": {
    "constraints": {
      "npm": "= 7.19"
    }
  }
}
